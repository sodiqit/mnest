{
    "name": "mnest",
    "version": "0.0.1",
    "description": "A CLI to bootstrap Metalamp nest js starter kit",
    "main": "src/index.js",
    "bin": {
        "mnest": "bin/mnest"
    },
    "files": [
        "bin/",
        "dist/"
    ],
    "scripts": {
        "prebuild": "rm -rf dist",
        "build:dev": "yarn prebuild && rollup -c rollup.config.dev.js",
        "start:dev": "nodemon --watch ./src --exec 'yarn build:dev' -e ts"
    },
    "repository": "git://github.com/sodiqit/mnest.git",
    "keywords": [
        "cli",
        "nestjs",
        "metalamp"
    ],
    "author": "sodiqit",
    "license": "MIT",
    "dependencies": {
        "arg": "^5.0.1",
        "inquirer": "^8.2.0"
    },
    "devDependencies": {
        "@rollup/plugin-typescript": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-import-order": "^2.1.4",
        "eslint-plugin-prettier": "^4.0.0",
        "nodemon": "^2.0.14",
        "prettier": "^2.3.2",
        "rollup": "^2.58.3",
        "rollup-plugin-terser": "^7.0.2",
        "ts-loader": "^8.0.18",
        "typescript": "^4.3.5"
    }
}
